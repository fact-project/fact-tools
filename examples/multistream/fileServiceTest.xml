<container>


    <!-- Pfade zu den fits files. -->
    <property name="whitelist" value="/Users/kaibrugge/Development/BA/isdc/temp/20131012.json" />

    <property name="num_copies" value="2" />

    <service id="auxFileService" class="fact.auxservice.AuxFileService" auxFolder="file:/Users/kaibrugge/fact_phido/aux/2013/10/12/" />

    <stream id="input:${copy.id}" class="fact.io.FactFileListMultiStream"
            listUrl="file:${whitelist}" copies="${num_copies}" suffix="fits.gz">
        <stream class="fact.io.FitsStream" id="fact" limit="10"/>
    </stream>


    <process input="input:${copy.id}" copies="${num_copies}" >

        <fact.filter.DrsCalibration key="Data" outputKey="DataCalibrated"/>
        <fact.features.source.SourcePosition outputKey="sourcePosition" auxService="auxFileService"/>
        <!--<fact.features.source.SourcePosition outputKey="sourcePosition" auxService="auxWebService"/>-->
        <PrintData/>
        <!--<fact.ShowViewer key="DataCalibrated" />-->
    </process>


</container>

