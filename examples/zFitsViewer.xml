	<property name="infile" value="/home/fabian/mount/Phido/fhgfs_app/fact/raw/2013/10/27/20131027_153.fits.fz" />
	<property name="drsfile" value="/home/fabian/mount/Phido/fhgfs_app/fact/raw/2013/10/27/20131027_148.drs.fits.gz" />
	
	<!-- Name des Streams und pfad zu dem Daten-File -->
