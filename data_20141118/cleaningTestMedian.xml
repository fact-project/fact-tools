<fact.cleaning.TwoLevelTimeMedian
    photonChargeKey="%{data.pc_key}"
    outputKey="%{data.cleaning_key}"
    arrivalTimeKey="${at_key}"
    starPositionKeys="Cetatauri"
    starRadiusInCamera="11.0"
    corePixelThreshold="6"
    neighborPixelThreshold="3"
    timeThreshold="30"
    minNumberOfPixel="2"
/>
<fact.features.NumberOfPixelInShower
    showerKey="%{data.cleaning_key}"
    outputKey="N_%{data.cleaning_key}"
/>
<fact.features.Size
    showerKey="%{data.cleaning_key}"
    photonChargeKey="%{data.pc_key}"
    outputKey="S_%{data.cleaning_key}"
/>
<fact.features.NumberOfIslands
    key="%{data.cleaning_key}"
    outputKey="nI_%{data.cleaning_key}"
/>
<fact.statistics.ArrayStatistics
    key="%{data.pc_key}"
    outputKey="pC_%{data.cleaning_key}"
    pixelSetKey="%{data.cleaning_key}"
/>
<fact.statistics.ArrayStatistics
    key="%{data.at_key}"
    outputKey="aT_%{data.cleaning_key}"
    pixelSetKey="%{data.cleaning_key}"
/>
</module>